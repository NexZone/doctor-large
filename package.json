{
	"name": "doctor-large",
	"version": "1.0.0",
	"description": "He's here.",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"prebuild": "npx rimraf dist",
		"build": "tsc",
		"precommands:clear": "npm run build",
		"commands:clear": "node -r dotenv/config scripts/clearCommands.js",
		"precommands:put": "npm run build",
		"commands:put": "node -r dotenv/config scripts/putCommands.js",
		"format": "prettier --write \"**/*.+(js|ts|json)\"",
		"lint:fix": "eslint src/** --ext .ts --fix",
		"lint": "eslint src/** --ext .ts",
		"prestart": "npm run build",
		"start": "node -r dotenv/config ."
	},
	"author": "Drango",
	"license": "MIT",
	"dependencies": {
		"@drango/tag-functions": "^1.0.9",
		"@prisma/client": "^4.11.0",
		"common-tags": "^1.8.2",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.3",
		"ms": "^2.1.3"
	},
	"devDependencies": {
		"@types/common-tags": "^1.8.1",
		"@types/eslint": "^8.21.3",
		"@types/ms": "^0.7.31",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"discord-api-types": "^0.37.37",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"prettier": "^2.8.7",
		"prisma": "^4.11.0",
		"typescript": "^5.0.2"
	},
	"imports": {
		"#components": "./dist/components/index.js",
		"#constants": "./dist/constants.js",
		"#database/*": "./dist/database/*",
		"#helpers/*": "./dist/helpers/*",
		"#logger": "./dist/logger/logger.js",
		"#modules/*": "./dist/modules/*",
		"#scripts/*": "./dist/scripts/*",
		"#typings": "./dist/typings/index.d.js"
	}
}
