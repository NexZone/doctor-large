generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model autorole {
  guildId          String   @id
  activated        Boolean  @default(false)
  roleIds          String[] @default([])
  moderatorTag     String?
  moderatorId      String?
  discordTimestamp String?
}
